import com.well.modules.models.Meeting;
import com.well.modules.models.chat.ChatMessage;

CREATE TABLE ChatContentMeetings (
    messageId INTEGER AS ChatMessage.Id NOT NULL PRIMARY KEY,
    meetingId INTEGER AS Meeting.Id NOT NULL
);

insert:
INSERT INTO ChatContentMeetings(messageId, meetingId)
VALUES (?, ?);

getById:
SELECT * FROM ChatContentMeetings WHERE messageId = ?;